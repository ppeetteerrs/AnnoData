{
  "name": "annodata-frontend",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "antd": "^3.15.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "electron-is-dev": "^1.0.1",
    "electron-packager": "^13.1.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "popper.js": "^1.14.7",
    "react": "^16.8.3",
    "react-art": "^16.8.6",
    "react-dom": "^16.8.3",
    "react-native": "^0.59.2",
    "react-native-gesture-handler": "^1.1.0",
    "react-native-web": "^0.11.2",
    "react-navigation": "^3.2.3",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "react-thunk": "^1.0.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "typescript": "^3.3.3"
  },
  "scripts": {
    "dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-pack": "build -c.extraMetadata.main=build/electron.js"
  },
  "build": {
    "appId": "com.peteryuen.annodata",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "./",
  "author": "Peter Yuen",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@types/jest": "^24.0.6",
    "@types/lodash": "^4.14.122",
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/react-navigation": "^3.0.3",
    "@types/react-redux": "^7.0.1",
    "@types/react-router": "^4.4.4",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack-env": "^1.13.8",
    "concurrently": "^4.1.0",
    "electron": "^11.5.0",
    "electron-builder": "^20.38.5",
    "wait-on": "^3.2.0"
